@model IEnumerable<DriverViewModel>

@{
    Layout = "_NewLayout";
}

<div class="container">

    <div id="tab">
        <table id="example" class="table table-striped table-hover" style="width:100%">
            <thead>
                <tr>
                    <th></th>
                    <th></th>
                    <th></th>
                    <th></th>

                </tr>
            </thead>
            <tbody>

                @foreach (var item in Model)
                {
                    <tr>

                        <td>@item.ApplicationUsers.FirstName</td>
                        <td>@item.CabName</td>
                        <td>@item.CabType</td>
                        <td>
                            <button class="btn btn-outline-info" onclick="hidetable()">View Details</button>
                        </td>

                    </tr>

                }

            </tbody>
        </table>
    </div>



    <div>


        <ul class="nav nav-tabs" role="tablist">
            <li class="nav-item" role="presentation">
                <a class="nav-link  active" data-bs-toggle="tab" href="#home" aria-selected="false" role="tab" tabindex="-1">Personal Details</a>
            </li>
            <li class="nav-item" role="presentation">
                <a class="nav-link " data-bs-toggle="tab" href="#profile" aria-selected="true" role="tab">Cab Details</a>
            </li>

     
        </ul>
        <div id="myTabContent" class="tab-content">
            <div class="tab-pane fade" id="home" role="tabpanel">
                <div class="d-flex justify-content-center">
                    <img src="https://www.sainik.telangana.gov.in/images/rswo-profilepic.png" alt="" class="img-fluid rounded-circle" width="200px">
                </div>
                <div>
                        <table class="table table-hover table-striped ">

                            <tr>
                            <td>First Name</td>
                            <td>:</td>
                            <td>Value</td>
                            </tr>

                             <tr>
                            <td>Last Name</td>
                            <td>:</td>
                            <td>Value</td>
                            </tr>

                             <tr>
                            <td>Emai Id</td>
                            <td>:</td>
                            <td>Value</td>
                            </tr>

                             <tr>
                            <td>Phone</td>
                            <td>:</td>
                            <td>Value</td>
                            </tr>

                             <tr>
                            <td>House Number</td>
                            <td>:</td>
                            <td>Value</td>
                            </tr>

                              <tr>
                            <td>Locality</td>
                            <td>:</td>
                            <td>Value</td>
                            </tr>

                        <tr>
                        <td>District</td>
                            <td>:</td>
                            <td>Value</td>
                        </tr>
                              <tr>
                            <td>State</td>
                            <td>:</td>
                            <td>Value</td>
                            </tr>



                        </table>
                        
                 

                </div>
            </div>
            <div class="tab-pane fade active show" id="profile" role="tabpanel">
                <table>


                    <tr class="table table-hover table-striped ">
                    <td>Licence Number</td>
                    <td>:</td>
                    <td>Value</td>
                    </tr>

                      <tr>
                            <td>Registartion Of Vehicles Number</td>
                            <td>:</td>
                            <td>Value</td>
                            </tr>


                    <tr>
                    <td>Cab Type</td>
                    <td>:</td>
                    <td>Value</td>
                    </tr>

                    <tr>
                    <td>Cab Name</td>
                    <td>:</td>
                    <td>Value</td>
                    </tr>

                </table>
            </div>
        </div>


    </div>
   

</div>

